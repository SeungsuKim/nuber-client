  },
  "devDependencies": {
    "@types/jest": "^24.0.14",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-toastify": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "apollo": "^2.13.1",
    "typescript": "^3.5.2"
  }
}
